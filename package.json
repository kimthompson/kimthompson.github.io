{
  "name": "kimthompson.me",
  "description": "I love that GitHub still hosts for free <3",
  "author": "Kim Thompson",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "parcel src/index.pug",
    "build": "parcel build src/index.pug --public-url ./ --out-dir docs",
    "pre-publish": "cp CNAME docs/CNAME",
    "publish": "git subtree push --prefix docs origin master",
    "clean": "rm -r dist; rm -r .cache; rm -r docs; rm -r node_modules; npm cache verify"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "swing": "^3.1.4"
  },
  "devDependencies": {
    "parcel": "^1.12.3",
    "pug": "^2.0.4",
    "stylus": "^0.54.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimthompson/drag-race-fantasy.git"
  },
  "bugs": {
    "url": "https://github.com/kimthompson/drag-race-fantasy/issues"
  }
}
